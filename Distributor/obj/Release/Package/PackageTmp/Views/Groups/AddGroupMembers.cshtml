@model Distributor.ViewModels.GroupAddView


@for(int i = 0; i < Model.Members.Count; i++)
{
    @Html.HiddenFor(m => m.Members[i].ReferenceName)
    @Html.HiddenFor(m => m.Members[i].ReferenceId)
    <tr>
        <td>@Html.CheckBoxFor(m => m.Members[i].SelectedUser)</td>
        <td>@Html.DisplayFor(m => m.Members[i].ReferenceName)</td>
    </tr>
}